{
  "name": "gulp-inject",
  "version": "4.3.2",
  "description": "A javascript, stylesheet and webcomponent injection plugin for Gulp, i.e. inject file references into your index.html",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/klei/gulp-inject",
  "bugs": "https://github.com/klei/gulp-inject/issues",
  "author": {
    "name": "Joakim Carlstein",
    "email": "joakim@klei.se",
    "url": "http://joakim.beng.se"
  },
  "keywords": [
    "gulpplugin",
    "inject",
    "stylesheets",
    "webcomponents",
    "scripts",
    "index"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/klei/gulp-inject.git"
  },
  "scripts": {
    "lint": "xo",
    "pretest": "npm run -s lint",
    "test": "mocha -R spec src/**/*_test.js",
    "semantic-release": "semantic-release",
    "commitmsg": "commitlint -e"
  },
  "dependencies": {
    "ansi-colors": "^1.1.0",
    "arrify": "^1.0.1",
    "escape-string-regexp": "^1.0.5",
    "event-stream": "^3.3.4",
    "fancy-log": "^1.3.2",
    "group-array": "^0.3.3",
    "plugin-error": "^1.0.1",
    "stream-to-array": "^2.3.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^3.2.0",
    "@commitlint/config-angular": "^3.1.1",
    "husky": "^0.14.3",
    "mocha": "^5.1.1",
    "semantic-release": "^15.2.0",
    "should": "^13.2.1",
    "strip-color": "^0.1.0",
    "vinyl": "^2.1.0",
    "xo": "^0.20.3"
  },
  "engines": {
    "node": ">=4"
  },
  "xo": {
    "space": true,
    "envs": [
      "node"
    ],
    "rules": {
      "func-names": 0,
      "func-name-matching": 0,
      "node/no-deprecated-api": 0,
      "no-buffer-constructor": 0,
      "no-multi-assign": 0,
      "no-unused-vars": 0,
      "no-useless-escape": 0,
      "no-var": 0,
      "object-shorthand": [
        2,
        "never"
      ],
      "prefer-arrow-callback": 0,
      "unicorn/explicit-length-check": 0,
      "unicorn/filename-case": 0,
      "unicorn/import-index": 0
    }
  }
}
